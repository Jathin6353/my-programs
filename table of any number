#include <stdio.h>

int main() {
    int num, i;

    /* Ask user for input */
    printf("Enter a number: ");
    scanf("%d", &num);

    /* Print table of the given number */
    printf("Table of %d:\n", num);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i);
    }

    return 0;
}
